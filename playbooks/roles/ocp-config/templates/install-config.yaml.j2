apiVersion: v1
baseDomain: {{ install_config.cluster_domain }}
compute:
- hyperthreading: Enabled
  name: worker
  platform: {}
  replicas: 0
controlPlane:
  hyperthreading: Enabled
  name: master
  platform: {}
  replicas: {{ master_count }}
metadata:
  creationTimestamp: null
  name: {{ install_config.cluster_id }}
networking:
  clusterNetwork:
  - cidr: 10.128.0.0/14
    hostPrefix: 23
  networkType: OpenShiftSDN
  serviceNetwork:
  - {{ install_config.servicenetwork}}
platform:
  none: {}
pullSecret: '{{ install_config.pull_secret }}'
sshKey: '{{ install_config.public_ssh_key }}'
{% if proxy_url != "" %}
proxy:
  httpProxy: {{ proxy_url }}
  httpsProxy: {{ proxy_url }}
  noProxy: {{ install_config.cluster_domain }},10.128.0.0/14,172.30.0.0/16,{{ no_proxy }}
{% endif %}
